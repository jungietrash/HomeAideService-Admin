<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.testing01.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_chat"
        android:name="com.example.testing01.ui.chat.ChatFragment"
        android:label="@string/title_chat"
        tools:layout="@layout/activity_chat" />

    <fragment
        android:id="@+id/navigation_booking"
        android:name="com.example.testing01.ui.booking.BookingFragment"
        android:label="@string/title_booking"
        tools:layout="@layout/fragment_booking" />

    <fragment
        android:id="@+id/navigation_setting"
        android:name="com.example.testing01.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/activity_setting" />

</navigation>